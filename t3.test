#include <stdio.h>
#include <stdlib.h>

int main(){
	
	// testing more global prediction, tests AND relationship, linearly separable
	int x = 1000;
	while(x > 0){
		int i = rand() % 2;
		int j = rand() % 3;
		if(i == 1){
			printf("i");
		}
		if(j == 1){
			printf("j");
		}
		if (i == 1 && j == 1){
			printf("i and j");
		}
		x--;
	}

	return 0;
}